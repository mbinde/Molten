					<key>delay</key>
					<real>0.1</real>
				</dict>
			</array>
			<key>description</key>
			<string>Custom pattern for delete actions</string>
		</dict>
		<key>refreshPattern</key>
		<dict>
			<key>type</key>
			<string>custom</string>
			<key>pattern</key>
			<array>
				<dict>
					<key>eventType</key>
					<string>impact</string>
					<key>intensity</key>
					<real>0.3</real>
					<key>delay</key>
					<real>0.0</real>
				</dict>
				<dict>
					<key>eventType</key>
					<string>impact</string>
					<key>intensity</key>
					<real>0.5</real>
					<key>delay</key>
					<real>0.05</real>
				</dict>
				<dict>
					<key>eventType</key>
					<string>impact</string>
					<key>intensity</key>
					<real>0.7</real>
					<key>delay</key>
					<real>0.1</real>
				</dict>
			</array>
			<key>description</key>
			<string>Progressive pattern for refresh actions</string>
		</dict>
	</dict>
