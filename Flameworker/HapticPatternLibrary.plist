			<key>type</key>
			<string>selection</string>
			<key>description</key>
			<string>Selection change feedback</string>
		</dict>
		<key>customDeletePattern</key>
		<dict>
			<key>type</key>
			<string>custom</string>
			<key>pattern</key>
			<array>
				<dict>
					<key>eventType</key>
					<string>impact</string>
					<key>intensity</key>
					<real>0.7</real>
					<key>delay</key>
					<real>0.0</real>
				</dict>
				<dict>
					<key>eventType</key>
					<string>impact</string>
					<key>intensity</key>
